{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    // override tslint-react rules here
    "jsx-wrap-multiline": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "variable-name": false,
    "no-console": [
      false
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": false,
    "member-access": false
  }
}

